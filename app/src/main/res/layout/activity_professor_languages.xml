<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.ProfessorLanguagesActivity">

    <EditText
        android:hint="Profesor Id"
        android:id="@+id/etProfessorIDLenguajes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:hint="Lenguaje Id"
        android:id="@+id/etLenguajesIDLenguajes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Salvar"
        android:id="@+id/btnProfessorLenguajesGuardar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Leer Profesores"
        android:id="@+id/btnProfessorLenguajesConsultar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Leer Lenguajes"
        android:id="@+id/btnLenguajesLenguajesConsultar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>